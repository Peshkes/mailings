{
  "name": "mailings",
  "version": "1.0.0",
  "description": "Small-little mailing app",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "build": "webpack --config webpack.config.js --mode development",
    "dev": "npm run build && npm run start",
    "make": "electron-forge make",
    "make-win": "electron-forge make --platform=win32 --arch=x64"
  },
  "keywords": [],
  "author": "peshkes",
  "license": "ISC",
  "type": "module",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-zip",
          "config": {
            "platforms": [
              "win32"
            ]
          }
        }
      ]
    }
  },
  "devDependencies": {
